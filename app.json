{
  "expo": {
    "name": "UpdatesNativeDebugTest",
    "slug": "UpdatesNativeDebugTest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "updatesnativedebugtest",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.douglowderexpo.UpdatesNativeDebugTest"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.douglowderexpo.UpdatesNativeDebugTest"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-updates",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "054cb504-1a32-4df1-bdf1-495cbf80686e"
      }
    },
    "owner": "doug-expo",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "requestHeaders": {
        "expo-channel-name": "local"
      },
      "useNativeDebug": true,
      "url": "https://u.expo.dev/054cb504-1a32-4df1-bdf1-495cbf80686e"
    }
  }
}
